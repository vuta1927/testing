networks:
  mapnet:
    external:
      name: mapnet
services:
  proxy:
    build:
      context: E:\GitHub\testing\Demo\Proxy
      dockerfile: Dockerfile
    container_name: maptemplate.nginx-proxy
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: proxy
    links:
    - webapi
    networks:
      mapnet: null
    ports:
    - 8080:80/tcp
  webapi:
    build:
      context: E:\GitHub\testing\Demo
      dockerfile: WebApi/Dockerfile
    container_name: maptemplate.api
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    expose:
    - '5000'
    image: webapi
    networks:
      mapnet:
        ipv4_address: 10.0.0.10
    ports:
    - 5000:5000/tcp
version: '3.0'